{
  "name": "workers",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js", 
  "types": "dist/index.d.ts", 
  "scripts": {
    "compile": "tsc -p tsconfig.worker.json && node ../../scripts/resolve-aliases.mjs",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "start:message": "node dist/workers/messageProcessor.js",
    "start:sequence": "node dist/workers/sequenceStepProcessor.js",
    "dev:message": "tsx src/workers/messageProcessor.ts",
    "dev:sequence": "tsx src/workers/sequenceStepProcessor.ts"
  },
  "dependencies": {
    "bullmq": "^5.45.2",
    "ioredis": "^5.6.0",
    "@meuprojeto/shared-lib": "workspace:*"
  },
  "devDependencies": {
    "tsx": "^4.17.0",
    "typescript": "^5.8.2",
    "@types/node": "^20",
    "npm-run-all": "^4.1.5"
  }
}